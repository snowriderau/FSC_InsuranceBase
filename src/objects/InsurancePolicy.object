    <fields>
        <fullName>INS_Policy_Term__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(
ROUND((NULLVALUE(ExpirationDate,NOW()) - NULLVALUE(EffectiveDate,NOW())),1)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
